<% include partials/head %>
<body>
<div id="wrapper">
    <% include partials/navbar %>

    <!-- PAGE TITLE START -->
    <section id="title" class="container-fluid wow fadeInDown">
        <div class="container">
            <div class="row">
                <div class="col-xs-6">
                    <h1>Register</h1>
                </div>
                <div class="col-xs-6 text-right breadcrumbs">
                    <ul class="list-inline list-unstyled">
                        <li><a href="service">Home</a></li>
                        <li>/</li>
                        <li>Register</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <!-- PAGE TITLE END -->

   <section id="home-content">
       <!-- CONTENT START -->
       <div class="col-2 ">
           <div class="container">

           </div>
       </div>
       <div class="container padding15">
           <div id="signup">
               <form method="POST" action="/register" enctype="multipart/form-data">
                   <div class="row">
                       <div class="col-sm-12 col-md-12 col-lg-2">
                           <div class="form-group">
                               <h4>ข้อมูลส่วนตัว</h4>
                           </div>
                       </div>
                   </div>
                   <div class="row">
                       <div class="col-sm-12 col-md-6 col-lg-6">
                           <div class="form-group">
                               <label for="name">ชื่อ (ภาษาไทย)</label>
                               <input id="name" name="name" class="form-control" type="text" required>
                           </div>
                           <div class="form-group">
                               <label for="lname">นามสกุล (ภาษาไทย)</label>
                               <input id="lname" name="lname" class="form-control" type="text" required>
                           </div>
                           <div class="form-group">
                               <label for="gender">เพศ</label>
                               <select class="form-control" id="gender" name="gender">
                                   <option value="ชาย">ชาย</option>
                                   <option value="หญิง">หญิง</option>
                               </select>
                           </div>
                       </div>
                       <div class="col-sm-12 col-md-6 col-lg-6">
                           <div id="uploadUserPic">
                               <div class="form-group">
                                <span><label>รูปภาพควรมีขนาด 640px X 480px</label>
                            <br>
                            <div style="width: 100%;height: 180px;">
                                <img id="img-upload" style="width: 240px ;height: 180px;">
                            </div></span>
                                   <input type="file" id="pic" name="pic" required>
                               </div>
                           </div>
                       </div>
                   </div>
                   <div class="row">
                       <div class="col-sm-12 col-md-12 col-lg-2">
                           <div class="form-group">
                               <h4>ที่อยู่ปัจจุบัน</h4>
                           </div>
                       </div>
                   </div>
                   <div class="row">
                       <div class="col-sm-12 col-md-12 col-lg-2">
                           <div class="form-group">
                               <label for="address">บ้านเลขที่</label>
                               <input id="address" name="address" class="form-control" type="text" required>
                           </div>
                       </div>
                       <div class="col-sm-12 col-md-12 col-lg-5">
                           <div class="form-group">
                               <label for="village">หมู่/หมู่บ้าน</label>
                               <input id="village" name="village" class="form-control" type="text" value="-">
                           </div>
                       </div>
                       <div class="col-sm-12 col-md-12 col-lg-5">
                           <div class="form-group">
                               <label for="road">ถนน</label>
                               <input id="road" name="road" class="form-control" type="text" value="-">
                           </div>
                       </div>
                   </div>


                   <div class="row">
                       <div class="col-sm-12 col-md-12 col-lg-2">
                           <div class="form-group">
                               <label for="subdistrict">ตำบล/แขวง</label>
                               <input id="subdistrict" name="subdistrict" type="text" class="form-control" required>
                           </div>
                       </div>
                       <div class="col-sm-12 col-md-12 col-lg-5">
                           <div class="form-group">
                               <label for="district">อำเภอ/เขต</label>
                               <input id="district" name="district" type="text" class="form-control" required>
                           </div>
                       </div>
                       <div class="col-sm-12 col-md-12 col-lg-5">
                           <div class="form-group">
                               <label for="province">จังหวัด</label>
                               <input id="province" name="province" type="text" class="form-control" required>
                           </div>
                       </div>
                   </div>


                   <div class="row">
                       <div class="col-sm-12 col-md-12 col-lg-2">
                           <div class="form-group">
                               <label for="zipcode">รหัสไปรษณีย์</label>
                               <input id="zipcode" name="zipcode" type="text" class="form-control" required>
                           </div>
                       </div>
                       <div class="col-sm-12 col-md-12 col-lg-5">
                           <div class="form-group">
                               <label for="phone">เบอร์โทรศัพท์</label>
                               <input id="phone" name="phone" type="text" class="form-control" required>
                           </div>
                       </div>
                       <div class="col-sm-12 col-md-12 col-lg-5">
                           <div class="form-group">
                               <label for="username">username</label>
                               <input id="username" name="username" type="text" class="form-control" required>
                           </div>
                       </div>
                   </div>
                   <div class="row">
                       <div class="col-sm-12 col-md-12 col-lg-5">
                           <div class="form-group">
                               <label for="password">รหัสผ่าน</label>
                               <input id="password" name="password" type="password" class="form-control" required>
                           </div>
                       </div>
                   </div>
                   <input type="text" id="status" name="status" value="consumer" hidden>
                   <div class="form-group">
                       <div align="center">
                           <label class="col-sm-4 control-label">
                           </label>
                       </div>
                       <div class="col-sm-8">
                           <button class="btn btn-success" href="">Submit</button>
                           <button class="btn btn-warning" type="clear">Clear</button>
                           <a class="btn btn-default" href="service">Back</a>
                       </div>
                   </div>
               </form>
           </div>
       </div>
   </section>
    <% include partials/footer %>
</div>
<!-- CONTENT END -->

<!-- MAIN JS FILES REQUIRED ON ALL PAGES -->
<script src="/vendor/jquery/jquery.min.js"></script>
<script src="/vendor/bootstrap-hover-dropdown/bootstrap-hover-dropdown.js"></script>
<script src="/vendor/bootstrap/js/bootstrap.min.js"></script>

<!-- EDURA JS REQUIRED ON ALL PAGES-->
<script src="/javascripts/script.js"></script>

</body>