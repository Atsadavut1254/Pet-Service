                                <div class="form-group">
                                    <input type="text" name="__pet_name"
                                           id="__pet_name" class="form-control"
                                           onblur="trimValue(this);"
                                           placeholder="ชื่อสุนัข" value="" required>
                                </div>
                            </div>
                            <div class="form-group">
                                <i class="fa fa-asterisk text-red"></i>
                            </div>
                        </div>
                    </div>
