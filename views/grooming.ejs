                        <div align="left">
                            <label class="col-sm-3 control-label">
                                ชื่อการจอง :
                            </label>
                        </div>
                        <div class="col-sm-5">
                            <input type="test" class="form-control" name="__grooming_wash_name"
                                   id="__grooming_wash_name" placeholder="ชื่อการจอง" required>
                            <div class="help-block with-errors"></div>
                        </div>
                        <i class="fa fa-asterisk text-red"></i>
                    </div>
                    <div class="row">
                        <div class="form-group">
                            <div align="left">
                                <label class="col-sm-3 control-label">
                                    วันที่จอง :
                                </label>
                            </div>
                            <div class="col-sm-3">
                                <input type="text" name="__grooming_wash_start_date"
                                       id="__grooming_wash_start_date" class="form-control daterangepicker"
                                       value="2019-05-29" placeholder="วันที่จอง" readonly
                                       required>
                            </div>
                            <div class="form-group">
                                <i class="fa fa-asterisk text-red"></i>
